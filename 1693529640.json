{
  "title": "numpad underscore",
  "rules": [
    {
      "description": "Numpad - to _",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": { "mandatory": ["left_shift"] },
            "key_code": "keypad_hyphen"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "hyphen",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    }
  ]
}
